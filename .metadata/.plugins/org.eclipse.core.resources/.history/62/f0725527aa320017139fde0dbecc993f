abstract class A
{
	void m1()
	{
		System.out.println("m1-A");
	
	}
	abstract void m2();
	abstract void m3();

}

class B extends A
{
	void m2()
	{
		System.out.println("m2-B");
	}
	void m3()
	{
		System.out.println("m3-B");
	}
	void m4()
	{
		System.out.println("m4-B");
		
	}
}

class First
{ 
	public static void main(String args[])
	{ 
	//A a = new A();
	A a1;
	}
}


